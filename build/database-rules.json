{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
   
    "private": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "public": {
      "todos": {
        ".read": "true",
        ".write": "true",
        ".indexOn": "created"
      }
    }
  }
}
